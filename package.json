{
  "name": "eslint-config-aurora-vue",
  "version": "3.0.5",
  "description": "ESLint configuration for Vue 3 + TS and for Vue 3 + TS + Feature-Sliced Design projects",
  "scripts": {
    "format:check": "prettier base/ --check",
    "format:fix": "prettier fsd/ --write",
    "test": "vitest"
  },
  "eslintConfig": {
    "base": "./configs/base/index.mjs",
    "vue": "./configs/vue-3/index.mjs",
    "fsd": "./configs/fsd/index.mjs"
  },
  "type": "module",
  "files": [
    "configs"
  ],
  "main": "configs/index.js",
  "types": "configs/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/rkorzhoff/eslint-config-vue"
  },
  "keywords": [
    "eslint",
    "vue",
    "vue3",
    "feature-sliced design"
  ],
  "author": "rkorzhoff",
  "license": "ISC",
  "dependencies": {
    "@eslint/compat": "^1.2.3",
    "@eslint/js": "^9.15.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/eslint__js": "^8.42.3",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/rule-tester": "^8.16.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "eslint": "9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-custom-alias": "^1.3.2",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-boundaries": "^5.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^2.0.4",
    "eslint-plugin-vue": "^9.31.0",
    "prettier": "^3.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vitest": "^2.1.6"
  },
  "devDependencies": {
    "@types/node": "^22.10.1"
  }
